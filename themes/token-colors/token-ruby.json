    "settings": {
      "foreground": "#8512a0"
    }
  },
  {
    "name": "Ruby 类变量",
    "scope": "variable.other.readwrite.class.ruby",
    "settings": {
      "foreground": "#8512a0"
    }
  },
  {
    "name": "Ruby 符号",
    "scope": "constant.other.symbol.ruby, punctuation.definition.constant.ruby",
    "settings": {
      "foreground": "#032F62"
    }
  },
  {
    "name": "Ruby 字符串",
    "scope": "string.quoted.single.ruby, string.quoted.double.ruby, string.quoted.double.interpolated.ruby",
