  "tokenColors": [
    {
      "name": "CMake 命令和实体",
      "scope": "entity.name.function.cmake, keyword.control.cmake",
      "settings": {
        "foreground": "#D73A49"
      }
    },
    {
      "name": "CMake 变量和字符串",
      "scope": "variable.parameter.cmake, string.unquoted.cmake",
      "settings": {
        "foreground": "#005CC5"
      }
    },
    {
      "name": "CMake 内部变量/属性",
      "scope": "variable.other.property.cmake",
      "settings": {
        "foreground": "#89B482"
      }
    },
    {
      "name": "CMake 源文件和字符串",
      "scope": "entity.source.cmake, string.source.cmake",
      "settings": {
        "foreground": "#A7BEC6"
      }
    },
    {
      "name": "CMake 存储类型",
      "scope": "storage.source.cmake",
      "settings": {
        "foreground": "#D73A49"
      }
    },
    {
      "scope": "comment, string.comment, punctuation.definition.comment",
      "settings": {
        "foreground": "#928374",
        "fontStyle": "italic"
      }
    },
    {
      "name": "C/C++ 函数调用、参数、数组括号",
      "scope": "meta.function-call.c, storage.modifier.array.bracket.square.c, meta.function.definition.parameters.c, meta.function-call.cpp, storage.modifier.array.bracket.square.cpp, meta.function.definition.parameters.cpp",
      "settings": {
        "foreground": "#24292E"
      }
    },
    {
      "name": "C/C++ 点访问符、行连续符",
      "scope": "punctuation.separator.dot-access.c, constant.character.escape.line-continuation.c, punctuation.separator.dot-access.cpp, constant.character.escape.line-continuation.cpp",
      "settings": {
        "foreground": "#24292E"
      }
    },
    {
      "name": "C 预处理指令",
      "scope": "keyword.control.directive.include.c, punctuation.definition.directive.c, keyword.control.directive.pragma.c, keyword.control.directive.line.c, keyword.control.directive.define.c, keyword.control.directive.conditional.c, keyword.control.directive.diagnostic.error.c, keyword.control.directive.undef.c, keyword.control.directive.conditional.ifdef.c, keyword.control.directive.endif.c, keyword.control.directive.conditional.ifndef.c, keyword.control.directive.conditional.if.c, keyword.control.directive.else.c",
      "settings": {
        "foreground": "#6F42C1"
      }
    },
    {
      "name": "C 指针访问符",
      "scope": "punctuation.separator.pointer-access.c",
      "settings": {
        "foreground": "#24292E"
      }
    },
    {
      "name": "C/C++ 成员变量",
      "scope": "variable.other.member.c, variable.other.member.cpp",
      "settings": {
        "foreground": "#24292E"
      }
    },
    {
      "name": "C++ 函数定义",
      "scope": "meta.function.definition.cpp",
      "settings": {}
